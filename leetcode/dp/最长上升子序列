1. 暴力求解会超时
2. 动态规划
```golang
func lengthOfLIS(nums []int) int {
    if len(nums) == 0 {return 0}
    dps := make([]int, len(nums))
    dps[0] = 1
    dp(dps, nums, 1)
    max := 0
    for i := 0;i < len(dps);i++ {
        if dps[i] > max {
            max = dps[i]
        }
    }
    return max
}

func dp(dps []int, nums[] int, i int) {
    if i >= len(nums) {
        return
    }
    dps[i] = 1
    for j := 0;j < i;j++ {
        if nums[j] < nums[i] && dps[j] + 1 > dps[i] {
            dps[i] = dps[j] + 1
        }
    }
    dp(dps, nums, i+1)
}
```
